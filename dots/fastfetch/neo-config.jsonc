        {
            "type": "wm",
            "key": "  ├ ",
            "keyColor": "blue"
        },
        {
            "type": "font",
            "key": "  ├ ",
            "keyColor": "blue",
            "format": "JetBrainsMono Nerd Font"
        },
        {
            "type": "terminal",
            "key": "  └ ",
            "keyColor": "blue"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└────────────────────────────────────────┘"
        },

        "break",

        {
            "type": "custom",
            "format": "\u001b[90m┌────────────Uptime / Age / DT───────────┐"
        },
        {
            "type": "command",
            "key": "  OS Age ",
            "keyColor": "magenta",
            "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
        },
        {
            "type": "uptime",
            "key": "  Uptime ",
            "keyColor": "magenta"
        },
        {
            "type": "datetime",
            "key": "  DateTime ",
            "keyColor": "magenta"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└────────────────────────────────────────┘"
        },

        "break",
